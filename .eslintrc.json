{
  "env": {
    "node": true,
    "es2021": true
  },
  "extends": "eslint:recommended",
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "import/no-anonymous-default-export": "off",
    "no-console": "warn",
    "prettier/prettier": [
      "warn",
      {
        "endOfLine": "auto",
        "singleQuote": true,
        "arrowParens": "avoid",
        "trailingComma": "all"
      }
    ]
  },
  "plugins": ["prettier"]
}
